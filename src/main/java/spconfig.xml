<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
 
<bean id="Ab" class="com.venky.AddressBean">  
<property name="Hno" value="7"></property>  
<property name="city" value="pragadavaram"></property> 
<property name="state" value="Ap"></property> 
</bean>   
<bean id="Sb" class="com.venky.EmployeeBean">  
<property name="id" value="101"></property>  
<property name="name" value="venky"></property> 
<property name="salary" value="50000"></property> 
<property name="addr" ref="Ab"></property>
</bean>    
</beans>
